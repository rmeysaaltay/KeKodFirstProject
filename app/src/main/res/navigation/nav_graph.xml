<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.projebir.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_happinesFragment"
            app:destination="@id/happinesFragment" />
        <action
            android:id="@+id/action_firstFragment_to_givingFragment"
            app:destination="@id/givingFragment" />
        <action
            android:id="@+id/action_firstFragment_to_kindnessFragment"
            app:destination="@id/kindnessFragment" />
        <action
            android:id="@+id/action_firstFragment_to_optimismFragment"
            app:destination="@id/optimismFragment" />
        <action
            android:id="@+id/action_firstFragment_to_respectFragment"
            app:destination="@id/respectFragment" />
    </fragment>
    <fragment
        android:id="@+id/givingFragment"
        android:name="com.example.projebir.GivingFragment"
        android:label="fragment_giving"
        tools:layout="@layout/fragment_giving" />
    <fragment
        android:id="@+id/happinesFragment"
        android:name="com.example.projebir.HappinesFragment"
        android:label="fragment_happiness"
        tools:layout="@layout/fragment_happiness" />
    <fragment
        android:id="@+id/kindnessFragment"
        android:name="com.example.projebir.KindnessFragment"
        android:label="fragment_kindness"
        tools:layout="@layout/fragment_kindness" />
    <fragment
        android:id="@+id/optimismFragment"
        android:name="com.example.projebir.OptimismFragment"
        android:label="fragment_optimism"
        tools:layout="@layout/fragment_optimism" />
    <fragment
        android:id="@+id/respectFragment"
        android:name="com.example.projebir.RespectFragment"
        android:label="fragment_respect"
        tools:layout="@layout/fragment_respect" />
</navigation>